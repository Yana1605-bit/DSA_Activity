#include <iostream>
#include <sstream>
#include <map>
using namespace std;

int main(){
	string words;
	cout <<"Enter a word/s:
	getline(cin, words);

	stringstream sstream(words);
	map<string, int> count;

	string word;
	while (sstream >> word){
		count[word]++;
	}
	cout << endl;

	for (const auto &c: count){
		cout << c.first <<"="<< c.second << endl;
	}

	return 0;
}
