#include <iostream>
#include <map>
#include <iostream>
using namespace std;

int main(){
	map <string, int> PeopleMap;
	PeopleMap.insert (pair<string, int> ("Alice", 15));
	PeopleMap.insert (pair<string, int> ("Bob", 20));
	PeopleMap.insert (pair<string, int> ("Charlie", 35));

	PeopleMap["Diana"] = 19;
	PeopleMap["Mae"] = 20;
	
	cout << "Names & Ages: " << endl;
	
	for (const auto &age : PeopleMap){
		cout << age.first << ", " << age.second << endl;
	}
	cout << endl;
	
	cout << "Alice, " << PeopleMap["Alice"] << endl;
	cout << "Yana, " << PeopleMap["Yana"] << endl;
	
	return 0;
}
